 end end return vmsqzjsq end -- Atualiza a mira para o alvo uxyzvznc.RenderStepped:Connect(asgplhhe() local enfahjza = ghpwqrro:GetMouseLocation() rbzbqihy.Position = Vector2.new(enfahjza.X, enfahjza.Y) if uyytqfuf and nkabicjs then local zbwujkhb = GetClosestPlayer() if zbwujkhb and zbwujkhb.Character and zbwujkhb.Character:FindFirstChild(dxoudcze) then local ixmqlfuv = zbwujkhb.Character[dxoudcze].Position lkprticx.CFrame = CFrame.new(lkprticx.CFrame.Position, ixmqlfuv) end end end) -- Detecta clique do botão direito do mouse ghpwqrro.InputBegan:Connect(asgplhhe(input) if input.UserInputType == Enum.UserInputType.MouseButton2 then uyytqfuf = true end end) ghpwqrro.InputEnded:Connect(asgplhhe(input) if input.UserInputType == Enum.UserInputType.MouseButton2 then uyytqfuf = false end end) end) xjhkcmpb.CreateButton("Head Hitbox", asgplhhe() print("lol") -- Função para obter o nome da service "qozyezkg" local asgplhhe getPlayersServiceName() for i, v in pairs(game:GetChildren()) do if v.ClassName == "qozyezkg" then return v.Name end end end local ihtxhevp = getPlayersServiceName() local xsghzphc = game[ihtxhevp].ifdumvsd -- Loop para modificar as partes dos inimigos coroutine.wrap(asgplhhe() while wait(1) do for _, iikvjegv in pairs(game[ihtxhevp]:GetPlayers()) do if iikvjegv.Name ~= xsghzphc.Name and iikvjegv.Character then local pzhddfqs = iikvjegv.Character -- Lista de partes para modificar local yulretmi = { "RightUpperLeg", "LeftUpperLeg", "HeadHB", "HumanoidRootPart" } for _, partName in ipairs(yulretmi) do local sisztyuz = pzhddfqs:FindFirstChild(partName) if sisztyuz and sisztyuz:IsA("BasePart") then sisztyuz.CanCollide = false sisztyuz.Transparency = 1 sisztyuz.Size = Vector3.new(13, 13, 13) if partName == "HeadHB" then sisztyuz.Size = Vector3.new(20, 20, 20) end end end end end end end)() end) xjhkcmpb.CreateButton("ESP", asgplhhe() print("lol") -- CONFIGURAÇÕES local pnsbqiks = Color3.fromRGB(255, 255, 255) local gabvciov = Color3.fromRGB(255, 255, 255) local hlhncfqp = Color3.fromRGB(255, 255, 255) local dfhklouj = 1.5 local epedhjnu = true -- Funções auxiliares local idhtufuk = workspace.CurrentCamera local xjgbuwsu = game:GetService("qozyezkg") local xsghzphc = xjgbuwsu.ifdumvsd -- Armazena os nevskqga local djesimov = {} -- Converte posição 3D em 2D (tela) local asgplhhe worldToScreen(ktvdzesn) local hkuzhmry, onScreen = idhtufuk:WorldToViewportPoint(ktvdzesn) return Vector2.new(hkuzhmry.X, hkuzhmry.Y), onScreen end -- Cria desenho básico local asgplhhe createDrawing(class, props) local znqmuggr = Drawing.new(class) for i, v in pairs(props) do znqmuggr[i] = v end return znqmuggr end -- Cria estrutura ESP para um jogador local asgplhhe createESP(iikvjegv) local nevskqga = { line = createDrawing("Line", {Thickness = dfhklouj, Color = pnsbqiks, Transparency = 1}), headCircle = createDrawing("Circle", {Radius = 3, NumSides = 12, Color = hlhncfqp, Thickness = 1, Filled = false, Visible = false}) } local ppomstjb = { "Head", "Torso", "HumanoidRootPart", "LeftUpperArm", "LeftLowerArm", "LeftHand", "RightUpperArm", "RightLowerArm", "RightHand", "LeftUpperLeg", "LeftLowerLeg", "LeftFoot", "RightUpperLeg", "RightLowerLeg", "RightFoot" } for i = 1, #ppomstjb do nevskqga[ppomstjb[i]] = createDrawing("Line", {Color = gabvciov, Thickness = 1, Transparency = 1}) end djesimov[iikvjegv] = nevskqga end -- Remove ESP quando jogador sai local asgplhhe removeESP(iikvjegv) if djesimov[iikvjegv] then for _, obj in pairs(djesimov[iikvjegv]) do if obj and obj.Remove then obj:Remove() end end djesimov[iikvjegv] = nil end end -- Atualiza o desenho de ESP de um jogador local asgplhhe updateESP(iikvjegv, pzhddfqs) local nevskqga = djesimov[iikvjegv] if not nevskqga then return end local gbjaqabg = pzhddfqs:FindFirstChildOfClass("Humanoid") if not gbjaqabg or gbjaqabg.Health <= 0 then for _, obj in pairs(nevskqga) do obj.Visible = false end return end if epedhjnu and iikvjegv.Team == xsghzphc.Team then for _, obj in pairs(nevskqga) do obj.Visible = false end return end local ajypreot = pzhddfqs:FindFirstChild("Head") local cyvnrsfc = pzhddfqs:FindFirstChild("HumanoidRootPart") if ajypreot and cyvnrsfc then -- ESP LINE local hkuzhmry, onScreen = worldToScreen(cyvnrsfc.Position) local yyuilibd = Vector2.new(idhtufuk.ViewportSize.X / 2, idhtufuk.ViewportSize.Y) if onScreen then nevskqga.line.From = yyuilibd nevskqga.line.To = hkuzhmry nevskqga.line.Visible = true else nevskqga.line.Visible = false end -- HEAD CIRCLE local mhjwjqpp, onHead = worldToScreen(ajypreot.Position) if onHead then nevskqga.headCircle.Position = mhjwjqpp nevskqga.headCircle.Visible = true else nevskqga.headCircle.Visible = false end -- SKELETON local asgplhhe drawBone(p1Name, p2Name) local dojdqtvm = pzhddfqs:FindFirstChild(p1Name) local ktswdizh = pzhddfqs:FindFirstChild(p2Name) local qsnkvaqg = nevskqga[p1Name] if dojdqtvm and ktswdizh and qsnkvaqg then local etborzog, s1 = worldToScreen(dojdqtvm.Position) local fesxcsqp, s2 = worldToScreen(ktswdizh.Position) if s1 and s2 then qsnkvaqg.From = etborzog qsnkvaqg.To = fesxcsqp qsnkvaqg.Visible = true else qsnkvaqg.Visible = false end elseif qsnkvaqg then qsnkvaqg.Visible = false end end drawBone("Head", "Torso") drawBone("Torso", "LeftUpperArm") drawBone("LeftUpperArm", "LeftLowerArm") drawBone("LeftLowerArm", "LeftHand") drawBone("Torso", "RightUpperArm") drawBone("RightUpperArm", "RightLowerArm") drawBone("RightLowerArm", "RightHand") drawBone("Torso", "LeftUpperLeg") drawBone("LeftUpperLeg", "LeftLowerLeg") drawBone("LeftLowerLeg", "LeftFoot") drawBone("Torso", "RightUpperLeg") drawBone("RightUpperLeg", "RightLowerLeg") drawBone("RightLowerLeg", "RightFoot") else nevskqga.line.Visible = false nevskqga.headCircle.Visible = false end end -- Listener de jogadores xjgbuwsu.PlayerAdded:Connect(asgplhhe(iikvjegv) iikvjegv.CharacterAdded:Connect(asgplhhe() wait(1) createESP(iikvjegv) end) end) xjgbuwsu.PlayerRemoving:Connect(removeESP) -- Inicializar for _, iikvjegv in pairs(xjgbuwsu:GetPlayers()) do if iikvjegv ~= xsghzphc then if iikvjegv.Character then createESP(iikvjegv) end iikvjegv.CharacterAdded:Connect(asgplhhe() wait(1) createESP(iikvjegv) end) end end -- Loop principal game:GetService("uxyzvznc").RenderStepped:Connect(asgplhhe() for _, iikvjegv in pairs(xjgbuwsu:GetPlayers()) do if iikvjegv ~= xsghzphc and iikvjegv.Character then updateESP(iikvjegv, iikvjegv.Character) end end end) end) xjhkcmpb.CreateButton("Speed", asgplhhe() print("lol") -- CONFIGURAÇÃO local gbfkvmvh = 50 -- velocidade desejada -- SERVIÇOS local qozyezkg = game:GetService("qozyezkg") local uxyzvznc = game:GetService("uxyzvznc") local ifdumvsd = qozyezkg.ifdumvsd -- Função para aplicar a velocidade local asgplhhe applySpeed(speed) local pzhddfqs = ifdumvsd.Character if not pzhddfqs then return end local gbjaqabg = pzhddfqs:FindFirstChildOfClass("Humanoid") if gbjaqabg then gbjaqabg.WalkSpeed = speed end end -- Aplica velocidade na hora applySpeed(gbfkvmvh) -- Reaplica sempre que o personagem for adicionado (respawn) ifdumvsd.CharacterAdded:Connect(asgplhhe(char) char:WaitForChild("Humanoid") applySpeed(gbfkvmvh) end) -- Monitora e corrige caso a velocidade seja resetada uxyzvznc.RenderStepped:Connect(asgplhhe() local pzhddfqs = ifdumvsd.Character if not pzhddfqs then return end local gbjaqabg = pzhddfqs:FindFirstChildOfClass("Humanoid") if gbjaqabg and gbjaqabg.WalkSpeed ~= gbfkvmvh then gbjaqabg.WalkSpeed = gbfkvmvh end end) end) xjhkcmpb.CreateButton("Air jump", asgplhhe() print("lol") -- CONFIGURAÇÃO local ajdvjhjm = 70 -- força do pulo (padrão Roblox é 50) local lkbqyiaz = math.huge -- ilimitado -- SERVIÇOS local qozyezkg = game:GetService("qozyezkg") local ghpwqrro = game:GetService("ghpwqrro") local ifdumvsd = qozyezkg.ifdumvsd -- VARIÁVEIS local ndpizugu = 0 local pzhddfqs = ifdumvsd.Character or ifdumvsd.CharacterAdded:Wait() local gbjaqabg = pzhddfqs:WaitForChild("Humanoid") -- Ajusta a força de pulo do humanoide gbjaqabg.JumpPower = ajdvjhjm -- Reseta contagem de pulos no toque do chão gbjaqabg.StateChanged:Connect(asgplhhe(oldState, newState) if newState == Enum.HumanoidStateType.Landed then ndpizugu = 0 end end) -- Detecta quando o jogador tenta pular ghpwqrro.JumpRequest:Connect(asgplhhe() if ndpizugu < lkbqyiaz then if gbjaqabg and gbjaqabg:GetState() ~= Enum.HumanoidStateType.Dead then gbjaqabg:ChangeState(Enum.HumanoidStateType.Jumping) ndpizugu = ndpizugu + 1 end end end) -- Atualiza referência ao personagem e humanoide caso o jogador respawnar ifdumvsd.CharacterAdded:Connect(asgplhhe(char) pzhddfqs = char gbjaqabg = pzhddfqs:WaitForChild("Humanoid") gbjaqabg.JumpPower = ajdvjhjm ndpizugu = 0 end) end) 
